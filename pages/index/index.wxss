swiper{
  height: calc(100vw * 340 / 750);
}
.swiper_container swiper swiper-item image{
  width: 100%;
}

.cate_container{
  width: 100%;
  display: flex;
}
.cate_container navigator{
  padding: 20rpx;
  flex:1;
}
.cate_container image{
  width: 100%;
}

.floor_title{
  padding: 10rpx 0;
}

.floor_title image{
  width: 100%;
}

.floor_content{
  overflow: hidden;
}

.floor_content navigator{
  float: left;
  width: 33.33%;
}

.floor_content navigator:nth-child(n+2){
  height: calc(250rpx * 386 / 232 / 2);
  border-left: 10rpx solid #fff;
}

.floor_content navigator:nth-child(2){
  border-bottom: 10rpx solid #fff;
}
.floor_content navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}

.floor_content navigator image{
  width: 100%;
  height: 100%;
}